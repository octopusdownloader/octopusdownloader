<BorderPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="89.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
            fx:controller="sample.NewDownloadController">
    <top>
        <HBox prefHeight="30.0" prefWidth="600.0" BorderPane.alignment="CENTER">
            <children>
                <Label prefHeight="17.0" prefWidth="573.0" text=" Enter new address to Download : ">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" top="10.0" />
                    </HBox.margin>
                </Label>
            </children>
        </HBox>
    </top>
    <left>
        <HBox alignment="TOP_CENTER" prefHeight="48.0" prefWidth="626.0" BorderPane.alignment="CENTER">
            <children>
                <TextField prefHeight="31.0" prefWidth="432.0" promptText="Enter the Address">
                    <HBox.margin>
                        <Insets right="10.0" />
                    </HBox.margin>
                </TextField>
                <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="60.0" text="OK">
                    <HBox.margin>
                        <Insets right="8.0" />
                    </HBox.margin>
                </Button>
                <Button mnemonicParsing="false" text="Cancel" />
            </children>
        </HBox>
    </left>
</BorderPane>
